<Configuration status="warn">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout
                pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p %c:%L - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="com.github.lburgazzoli.pulsar.function.wasm.support" level="WARN" additivity="false">
            <AppenderRef ref="console"/>
        </Logger>
        <Root level="INFO">
            <AppenderRef ref="console" />
        </Root>
    </Loggers>
</Configuration>